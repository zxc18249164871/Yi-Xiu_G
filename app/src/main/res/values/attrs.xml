<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTopBar">
        <!--左面按钮-->
        <attr name="LeftText" format="string"></attr>
        <attr name="LefrtTextSize" format="float"></attr>
        <attr name="LefrtTextGB" format="color|reference"></attr>
        <!--中间标题-->
        <attr name="TitleBar" format="string"></attr>
        <attr name="TitleTextSize" format="float"></attr>
        <attr name="TitleTextGB" format="color|reference"></attr>
        <!--右面按钮-->
        <attr name="RightText" format="string"></attr>
        <attr name="RightTextSize" format="float"></attr>
        <attr name="RightTextGB" format="color|reference"></attr>
    </declare-styleable>

</resources>
